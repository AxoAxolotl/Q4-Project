let total = 0; 

function completeOrder() {
  let thanks = Math.floor(Math.random() * 4);
  let events = Math.floor(Math.random() * 50);
  
  if (document.getElementById("orderResult").innerHTML == document.getElementById("currentBurger").innerHTML) {
    if (thanks == 0) {
      alert("That looks delicious! Thank you!")
    } else if (thanks == 1) {
      alert("Thanks!")
    } else if (thanks == 2) {
      alert("I can't wait to eat this!")
    } else if (thanks == 3) {
      alert("Yummy!")
    }
  orderLength = document.getElementById("currentBurger").innerHTML;
  let orderTotal = (orderLength.split(',').length - 1)*2;
  total += orderTotal; 
  document.getElementById("cash").innerHTML = "$" + total; 
    document.getElementById("currentBurger").innerHTML = "";
    nextOrder();
  } else {
    alert("That's wrong!")
    document.getElementById("currentBurger").innerHTML = "";
    nextOrder();
  }
  if (events == 0) {
    mouse();
  } else if (events == 1) {
    mold();
  } else if (events == 2) {
    robbery();
  } else if (events == 3) {
    angryCustomer();
  } else if (events == 4) {
    cleanUp();
  } else if (events == 5) {
    tip();
  }
  ranks();
}

function restockBeef() {
  if (total < 1) {
    alert("Not enough money!")
  } else {
  if (beefAmount == 5) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    beefAmount = 5;
  } else if (beefAmount == 4) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    total -= 4
    document.getElementById("cash").innerHTML = "$" + total;
    beefAmount = 5;
  } else if (beefAmount == 3) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    total -= 8
    document.getElementById("cash").innerHTML = "$" + total;
    beefAmount = 5;
  } else if (beefAmount == 2) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    total -= 12
    document.getElementById("cash").innerHTML = "$" + total;
    beefAmount = 5;
  } else if (beefAmount == 1) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    total -= 16
    document.getElementById("cash").innerHTML = "$" + total;
    beefAmount = 5;
  } else if (beefAmount == 0) {
    document.getElementById("beef").innerHTML = "Beef - 5";
    total -= 20
    document.getElementById("cash").innerHTML = "$" + total;
    beefAmount = 5;
  } 
  }
}

