    total -= 4
    document.getElementById("cash").innerHTML = "$" + total;
    mayoAmount = 5;
  } else if (mayoAmount == 0) {
    document.getElementById("mayo").innerHTML = "Mayo - 5";
    total -= 5
    document.getElementById("cash").innerHTML = "$" + total;
    mayoAmount = 5;
  } 
  }
}

