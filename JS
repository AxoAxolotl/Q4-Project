function backgroundGold() {
  if (total < 1000) {
    alert("Not enough money!")
  } else {
  total -= 1000;
  document.body.style.background = "linear-gradient(163deg, rgb(229 177 46) 0%, rgb(253 226 151) 35%, rgb(223 179 59) 100%)";
  document.getElementById("cash").innerHTML = "$" + total;
  ranks();
