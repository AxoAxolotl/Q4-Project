function ranks() {
  if (total < 1) {
    document.getElementById("yourRank").innerHTML = "Bankrupt";
    document.getElementById("yourRank").style.border = "2px solid black";
    document.getElementById("yourRank").style.backgroundColor = "#505050";
  } else if ((total > 1) && (total < 25)) {;
  document.getElementById("yourRank").innerHTML = "Broke"
  document.getElementById("yourRank").style.border = "2px solid black";
  document.getElementById("yourRank").style.backgroundColor = "#909090";
  } else if ((total > 25) && (total < 100)) {;
  document.getElementById("yourRank").innerHTML = "Poor"
  document.getElementById("yourRank").style.border = "2px solid black";
  document.getElementById("yourRank").style.backgroundColor = "#B5B5B5";
  } else if ((total > 100) && (total < 200)) {;
  document.getElementById("yourRank").innerHTML = "Bad"
  document.getElementById("yourRank").style.border = "2px solid black";
  document.getElementById("yourRank").style.backgroundColor = "#FFFFFF";
  } else if ((total > 200) && (total < 500)) {;
  document.getElementById("yourRank").innerHTML = "Okay"
  document.getElementById("yourRank").style.border = "2px solid black";
  document.getElementById("yourRank").style.backgroundColor = "#50C050";
  document.getElementById("yourRank").style.borderRadius = "5px";
  } else if ((total > 500) && (total < 1000)) {;
  document.getElementById("yourRank").innerHTML = "Average"
  document.getElementById("yourRank").style.border = "4px solid #E8B323";
  document.getElementById("yourRank").style.backgroundColor = "#50C050";
  document.getElementById("yourRank").style.borderRadius = "15px";
  } else if ((total > 1000) && (total < 3000)) {;
  document.getElementById("yourRank").innerHTML = "Good"
  document.getElementById("yourRank").style.border = "4px solid #E8B323";
  document.getElementById("yourRank").style.backgroundColor = "#9090E0";
  document.getElementById("yourRank").style.borderRadius = "15px";
  } else if ((total > 3000) && (total < 7500)) {;
  document.getElementById("yourRank").innerHTML = "Rich"
  document.getElementById("yourRank").style.border = "4px solid #E8B323";
  document.getElementById("yourRank").style.backgroundColor = "#9090E0";
  document.getElementById("yourRank").style.borderRadius = "15px";
  document.getElementById("yourRank").style.background = "linear-gradient(163deg, rgb(229 177 46) 0%, rgb(253 226 151) 35%, rgb(223 179 59) 100%)"
  } else if ((total > 7500) && (total < 15000)) {;
  document.getElementById("yourRank").innerHTML = "Wealthy"
  document.getElementById("yourRank").style.border = "4px solid #7D1CC7";
  document.getElementById("yourRank").style.backgroundColor = "#9090E0";
  document.getElementById("yourRank").style.borderRadius = "15px";
  document.getElementById("yourRank").style.background = "linear-gradient(163deg, rgb(229 177 46) 0%, rgb(253 226 151) 35%, rgb(223 179 59) 100%)"
  document.getElementById("yourRank").style.color = "#FF0000";
  } else if (total > 15000) {
  document.getElementById("yourRank").innerHTML = "Royalty"
  document.getElementById("yourRank").style.border = "4px solid #7D1CC7";
  document.getElementById("yourRank").style.backgroundColor = "#FF0000";
  document.getElementById("yourRank").style.borderRadius = "15px";
  document.getElementById("yourRank").style.background = "linear-gradient(163deg, rgb(229 177 46) 0%, rgb(253 226 151) 35%, rgb(223 179 59) 100%)"
  document.getElementById("yourRank").style.color = "#9090E0";
  }
}

ranks();

function backgroundWhite() {
  document.body.style.backgroundColor = "#FFFFFF";
  document.body.style.background = "rgba(0, 0, 0, 0)";
}

function backgroundBlue() {
  if (total < 20) {
    alert("Not enough money!")
  } else {
  total -= 20;
  document.getElementById("cash").innerHTML = "$" + total;
  document.body.style.background = "rgba(0, 0, 255, 1)";
  ranks();
  }
}

