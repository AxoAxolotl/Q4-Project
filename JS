function openVault() {
  let VaultCode = prompt("Enter Code");
  
  if (VaultCode == ":3") {
    let customCash = prompt("Enter Amount")
    total = total + parseInt(customCash)
    document.getElementById("cash").innerHTML = "$" + total;
    ranks();
  } else {
    alert("INCORRECT")
  }
}

function mouse() {
  cheeseAmount = 0;
  document.getElementById("cheese").innerHTML = "Cheese - 0";
  alert("A mouse has eaten all your cheese")
}

function mold() {
  bunAmount = 0;
  document.getElementById("buns").innerHTML = "Buns - 0";
  alert("All your buns got moldy")
}

function angryCustomer() {
  alert("Excuse me!")
  alert("My order was wrong!")
  alert("I asked for two pickles,")
  alert("But I only got one!")
  alert("I will sue you for this!")
  alert("You will be locked up for life!")
  alert("Maybe even death by firing squad!")
  alert("Apologize now!")
  let response = prompt("Give an apology")
  alert(response + " is not a valid answer!")
  alert("I am leaving to call the police!")
  alert("You'll never see me or my money again!")
  alert("G")
  alert("O")
  alert("O")
  alert("D")
  alert("B")
  alert("Y")
  alert("E")
  alert("!")
}

function robbery() {
  total -= Math.floor(Math.random() * 30) + 20;
  document.getElementById("cash").innerHTML = "$" + total;
  ranks();
  alert("You got robbed!")
}

function cleanUp() {
  total -= 20
  document.getElementById("cash").innerHTML = "$" + total;
  ranks();
  alert("There was a big mess, and you have to pay to clean it up.")
}

function tip() {
  total += Math.floor(Math.random() * 10);
  document.getElementById("cash").innerHTML = "$" + total;
  ranks();
  alert("You got a tip!")
}

function load() {
  localStorage.setItem("savedMoney", total);
}

function saving() {
  total = Number(localStorage.getItem("savedMoney"));
  document.getElementById("cash").innerHTML = "$" + total;
  ranks();
}
